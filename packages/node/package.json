{
  "name": "@doain/node",
  "version": "0.6.0",
  "description": "doain的Node端,主要提供运行命令",
  "keywords": [
    "doain",
    "admin-management",
    "element-plus",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckangwen/doain",
    "directory": "packages/node"
  },
  "license": "MIT",
  "author": "ckangwen",
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": "./*",
    "./global": {
      "types": "./global.d.ts"
    },
    "./config": {
      "types": "./config.d.ts",
      "require": "./dist/config.js",
      "import": "./dist/config.js"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "require": "./dist/plugin.js",
      "import": "./dist/plugin.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "global.d.ts",
    "config.d.ts",
    "plugin.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "@charrue/vite-plugin-pages": "^0.28.0",
    "@charrue/vite-plugin-pages-extend": "^0.1.0",
    "@charrue/vite-plugin-vue-layouts": "^0.8.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "compression": "^1.7.4",
    "debug": "^4.3.4",
    "minimist": "^1.2.7",
    "picocolors": "^1.0.0",
    "polka": "1.0.0-next.22",
    "rollup-plugin-visualizer": "^5.9.0",
    "sirv": "^2.0.2",
    "unocss": "^0.48.3",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^4.0.3",
    "vite-plugin-inspect": "^0.7.8"
  },
  "devDependencies": {
    "@charrue/toolkit": "^0.14.1",
    "@charrue/types": "^0.1.0",
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/escape-html": "^1.0.2",
    "@types/minimist": "^1.2.2",
    "@unocss/vite": "^0.48.3",
    "escape-html": "^1.0.3",
    "rollup": "^3.9.1",
    "type-fest": "^3.5.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
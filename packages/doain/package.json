{
  "name": "doain",
  "version": "0.4.0",
  "description": "类Vitepress架构的后台管理系统的框架,将琐碎的配置与工具函数封装与内部",
  "keywords": [
    "doain",
    "admin-management",
    "element-plus",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckangwen/doain",
    "directory": "packages/doain"
  },
  "license": "MIT",
  "author": "ckangwen",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.js"
    },
    "./*": "./*",
    "./config": {
      "types": "./config.d.ts",
      "require": "./config.cjs",
      "import": "./config.js"
    }
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "bin": {
    "doain": "bin/doain.js"
  },
  "files": [
    "dist",
    "bin",
    "*.d.ts",
    "*.mjs",
    "*.cjs"
  ],
  "dependencies": {
    "@doain/client": "workspace:latest",
    "@doain/node": "workspace:latest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}